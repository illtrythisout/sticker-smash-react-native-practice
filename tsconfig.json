{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/index.tsx",
    "app/+not-found.tsx",
    "app/(tabs)/about.tsx",
    "app/_layout.tsx",
    "app/(tabs)/_layout.tsx",
  ]
}
